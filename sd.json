{
  "software": {
    "components": {
      "metaconfig": {
        "services": {
          "_2fetc_2fbacula_2fbacula_2dsd_2econf": {
            "contents": {
              "main": {
                "Autochanger": [
                  {
                    "Changer_Command": "",
                    "Changer_Device": "/dev/null",
                    "Device": [
                      "dev0-1",
                      "dev0-2"
                    ],
                    "Name": "name-0"
                  },
                  {
                    "Changer_Command": "",
                    "Changer_Device": "/dev/null",
                    "Device": [
                      "dev1-1",
                      "dev1-2"
                    ],
                    "Name": "name-1"
                  }
                ],
                "Device": [
                  {
                    "AlwaysOpen": false,
                    "Archive_Device": "/dev/null",
                    "AutomaticMount": false,
                    "Device_Type": "Fifo",
                    "Label_Media": true,
                    "MaximumOpenWait": 60,
                    "Media_Type": "NULL",
                    "Name": "dummy",
                    "Random_Access": false,
                    "RemovableMedia": false
                  },
                  {
                    "Archive_Device": "/some/path",
                    "Autochanger": true,
                    "Autoselect": true,
                    "Device_Type": "File",
                    "Drive_Index": 0,
                    "Label_Media": true,
                    "Maximum_Block_Size": 524288,
                    "Maximum_Network_Buffer_Size": 262144,
                    "Media_Type": "DumpDisk1",
                    "Name": "vDrive-1-0",
                    "Random_Access": true
                  }
                ],
                "Director": [
                  {
                    "Monitor": true,
                    "Name": "director-short-sd-dir",
                    "Password": "@/etc/bacula/pw"
                  }
                ],
                "Messages": [
                  {
                    "Name": "standard",
                    "messagedestinations": [
                      {
                        "address": "director-short-sd-dir",
                        "destination": "director",
                        "types": [
                          "all",
                          "!skipped",
                          "!restored"
                        ]
                      }
                    ]
                  }
                ]
              }
            },
            "daemon": [
              "bacula-sd"
            ],
            "mode": 416,
            "module": "bacula/main"
          }
        }
      }
    }
  }
}
